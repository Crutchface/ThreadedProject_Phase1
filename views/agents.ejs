<%- include('./includes/head.ejs') %>
<body>

  <%- include('./includes/header.ejs') %>

  <%- include('./includes/nav-bar.ejs') %>


  <div class="content-container">
  

    <div class="agents">
    
      <% agency.forEach(location => { %>
      <h1>
        <%= location.AgncyCity %> <%= location.AgncyAddress %> <%=
        location.AgncyPhone %>
      </h1>
      <% agents.forEach(agent => { %> <% if(agent.AgencyId === location.AgencyID){
      %>
      
        <li>
        <%= agent.AgtFirstName %> <%= agent.AgtLastName %> <%= agent.AgtBusPhone
        %> <%= agent.AgtEmail %>
      </li>
        
        
      <% } %> <% }) %> <% }) %>
    </div>
    <br>
      
      <button class="button" onclick="editAgents()">Edit Agents</button>
      
     
    
  
  </div>

  <%- include('./includes/footer.ejs') %>

  
      
  
      <script>
        function editAgents() {
          window.location.href = "/editagents";
        }
      </script>
  
</body>
</html>

  
